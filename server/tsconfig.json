{
  "compilerOptions": {
    "outDir": "./lib",
    "module": "commonjs",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "downlevelIteration": true,
    "noEmitOnError": true,
    "strict": true,
    "experimentalDecorators": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {},
    "typeRoots": ["./node_modules/@types" ],
    "target": "es2017",
    "lib": ["dom", "es2017"]
  },
  "include": ["./src/**/*"],
  "exclude": []
}
